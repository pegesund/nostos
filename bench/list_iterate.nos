# List sum benchmark - 50M elements
# Tests tail-recursive sum with JIT optimization

# Sum with length verification (forces list materialization)
sumTR([], acc) = acc
sumTR([x | xs], acc) = sumTR(xs, acc + x)

main() = {
    list = range(1, 50000001)
    len = length(list)
    println(len)
    sum = sumTR(list, 0)
    println(sum)
    0
}
