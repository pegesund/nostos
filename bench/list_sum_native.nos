# List sum benchmark using native listSum
# Tests list traversal performance

# Build a list of n integers
range(0) = []
range(n) = [n | range(n - 1)]

main() = {
    # Create list of 1000 integers and sum 100 times
    lst = range(1000)
    var total = 0
    var i = 0
    while i < 100 {
        total = total + listSum(lst)
        i = i + 1
    }
    total
}
