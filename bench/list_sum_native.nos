# List sum benchmark - using native listSum builtin
# Tests native vs recursive fold performance

# Build a list of n integers
range(0) = []
range(n) = [n | range(n - 1)]

main() = {
    # Create list of 1000 integers and sum 100 times
    lst = range(1000)
    total = 0
    i = 0
    while i < 100 {
        total = total + listSum(lst)
        i = i + 1
    }
    total
}
