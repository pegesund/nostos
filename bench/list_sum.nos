# List sum benchmark - iterative style (like Python)
# Tests list traversal performance

sum(lst) = {
    acc = 0
    while !isEmpty(lst) {
        acc = acc + head(lst)
        lst = tail(lst)
    }
    acc
}

main() = {
    lst = rangeList(10000)
    total = 0
    i = 0
    while i < 100 {
        total = total + sum(lst)
        i = i + 1
    }
    total
}
