# expect: "passed"
type Result = Ok(Int) | Err(String) | Empty deriving Hash

main() = {
    ok1 = Ok(42)
    ok2 = Ok(42)
    ok3 = Ok(100)
    err = Err("fail")
    empty = Empty
    assert(hash(ok1) == hash(ok2))
    assert(hash(ok1) != hash(ok3))
    assert(hash(ok1) != hash(err))
    assert(hash(ok1) != hash(empty))
    "passed"
}
