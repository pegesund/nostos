# expect: "passed"
type Point = Point2D(Int, Int) | Point3D(Int, Int, Int) deriving Hash

main() = {
    p1 = Point2D(1, 2)
    p2 = Point2D(1, 2)
    p3 = Point2D(2, 1)
    p4 = Point3D(1, 2, 3)
    assert(hash(p1) == hash(p2))
    assert(hash(p1) != hash(p3))
    assert(hash(p1) != hash(p4))
    "passed"
}
