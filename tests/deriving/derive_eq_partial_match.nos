# expect: "passed"
type Triple = { a: Int, b: Int, c: Int } deriving Eq

main() = {
    t1 = Triple(1, 2, 3)
    t2 = Triple(1, 2, 3)
    t3 = Triple(0, 2, 3)
    t4 = Triple(1, 0, 3)
    t5 = Triple(1, 2, 0)
    assert(t1 == t2)
    assert(t1 != t3)
    assert(t1 != t4)
    assert(t1 != t5)
    "passed"
}
