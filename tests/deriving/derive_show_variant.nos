# expect: "passed"
# Test deriving Show for a variant type with multiple variants
type Result = Ok(Int) | Err(String) deriving Show
main() = {
    ok_val = Ok(42)
    err_val = Err("error")
    # Variant with value
    s1 = show(ok_val)
    assert_eq(s1, "Ok(42)")
    s2 = show(err_val)
    assert_eq(s2, "Err(error)")
    "passed"
}
