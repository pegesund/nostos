# expect: "passed"
# Test deriving Eq for a variant type
type Point = Point(Int, Int) deriving Eq
main() = {
    p1 = Point(3, 4)
    p2 = Point(3, 4)
    p3 = Point(5, 6)
    # Same points should be equal (using native == for now since trait Eq not fully working)
    assert(p1 == p2)
    # Different points should not be equal
    assert(!(p1 == p3))
    "passed"
}
