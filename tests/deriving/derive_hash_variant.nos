# expect: "passed"
# Test deriving Hash for a variant type with multiple variants
type Color = Red | Green | Blue | RGB(Int, Int, Int) deriving Hash
main() = {
    c1 = Red
    c2 = Red
    c3 = Green
    c4 = RGB(255, 128, 0)
    c5 = RGB(255, 128, 0)
    c6 = RGB(0, 0, 255)
    # Same variants should have same hash
    assert_eq(hash(c1), hash(c2))
    assert_eq(hash(c4), hash(c5))
    # Different variants should have different hash
    assert(hash(c1) != hash(c3))
    assert(hash(c4) != hash(c6))
    assert(hash(c1) != hash(c4))
    "passed"
}
