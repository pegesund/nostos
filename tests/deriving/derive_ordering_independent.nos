# expect: "passed"
type A = A(Int) deriving (Hash, Eq, Show, Copy)
type B = B(Int) deriving (Copy, Show, Eq, Hash)

main() = {
    a1 = A(42)
    a2 = A(42)
    b1 = B(42)
    b2 = B(42)

    assert(hash(a1) == hash(a2))
    assert(a1 == a2)
    a3 = copy(a1)
    assert(a3 == a1)

    assert(hash(b1) == hash(b2))
    assert(b1 == b2)
    b3 = copy(b1)
    assert(b3 == b1)

    "passed"
}
