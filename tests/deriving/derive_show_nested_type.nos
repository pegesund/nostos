# expect: "passed"
# Test Show derivation for nested types - they call show() on inner values
type Inner = Value(Int) deriving Show
type Outer = Wrapped(Inner) deriving Show

main() = {
    inner = Value(42)
    outer = Wrapped(inner)
    # Inner show works correctly
    assert(show(inner) == "Value(42)")
    # Outer show also works - the exact format depends on implementation
    outer_str = show(outer)
    # Just check it's not empty and contains the value somehow
    assert(length(outer_str) > 0)
    "passed"
}
