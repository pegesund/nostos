# expect: "passed"
# Test deriving multiple traits at once
type Vec2 = Vec2(Int, Int) deriving (Hash, Show, Eq)
main() = {
    v1 = Vec2(10, 20)
    v2 = Vec2(10, 20)
    v3 = Vec2(30, 40)
    # Test Hash
    assert_eq(hash(v1), hash(v2))
    assert(hash(v1) != hash(v3))
    # Test Show
    s = show(v1)
    assert_eq(s, "Vec2(10, 20)")
    # Test Eq (using native == since Eq trait implementation is native)
    assert(v1 == v2)
    assert(!(v1 == v3))
    "passed"
}
