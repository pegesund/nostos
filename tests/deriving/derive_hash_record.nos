# expect: "passed"
# Test deriving Hash for a variant type (positional fields)
# Hash is a built-in derivable trait - no explicit definition needed
type Point = Point(Int, Int) deriving Hash
main() = {
    p1 = Point(3, 4)
    p2 = Point(3, 4)
    p3 = Point(5, 6)
    h1 = hash(p1)
    h2 = hash(p2)
    h3 = hash(p3)
    # Same points should have same hash
    assert_eq(h1, h2)
    # Different points should have different hash
    assert(h1 != h3)
    "passed"
}
