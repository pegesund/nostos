# Test multiple decorators applied to one function
# expect: 42

template double(fn) = quote {
    result = ~fn.body
    result * 2
}

template triple(fn) = quote {
    result = ~fn.body
    result * 3
}

# Decorators are applied bottom-up: double first, then triple
# 7 * 2 = 14, 14 * 3 = 42
@triple
@double
get_value() = 7

main() = get_value()
