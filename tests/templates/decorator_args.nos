# Test decorator with arguments
# expect: 105

# Multiply result by given factor
template multiply(fn, factor) = quote {
    result = ~fn.body
    result * ~factor
}

# Add a fixed amount to result
template add(fn, amount) = quote {
    result = ~fn.body
    result + ~amount
}

# 7 * 10 = 70, then + 35 = 105
@add(35)
@multiply(10)
get_value() = 7

main() = get_value()
