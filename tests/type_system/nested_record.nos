# Nested record types

type Point = { x: Int, y: Int }
type Rectangle = { topLeft: Point, bottomRight: Point }

width(rect) = rect.bottomRight.x - rect.topLeft.x

main() = {
    p1: Point = Point(0, 0)
    p2: Point = Point(30, 20)
    rect: Rectangle = Rectangle(p1, p2)
    width(rect)
}
