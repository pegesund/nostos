# expect: 42
# Nested list patterns - now supported directly!

getFirstOfFirst([[x | _] | _]) = x
getFirstOfFirst(_) = 0

main() = {
    assert_eq(1, getFirstOfFirst([[1, 2], [3, 4]]))
    assert_eq(99, getFirstOfFirst([[99]]))
    assert_eq(0, getFirstOfFirst([[], [1]]))
    assert_eq(0, getFirstOfFirst([]))
    42
}
