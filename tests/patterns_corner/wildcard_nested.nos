# expect: 42
# Wildcard in nested positions

ignoreSecond((a, _, c)) = a + c
ignoreMiddle([a, _, c]) = a + c
ignoreMiddle(_) = 0

type Box = Box(Int, Int, Int)
boxFirst(Box(x, _, _)) = x

main() = {
    assert_eq(4, ignoreSecond((1, 999, 3)))
    assert_eq(4, ignoreMiddle([1, 999, 3]))
    assert_eq(0, ignoreMiddle([1, 2]))
    assert_eq(10, boxFirst(Box(10, 20, 30)))
    42
}
