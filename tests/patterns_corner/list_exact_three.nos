# expect: 42
# Exact three element list pattern

isThree([_, _, _]) = true
isThree(_) = false

sumThree([a, b, c]) = a + b + c
sumThree(_) = 0

main() = {
    assert_eq(true, isThree([1, 2, 3]))
    assert_eq(false, isThree([1, 2]))
    assert_eq(false, isThree([1, 2, 3, 4]))
    assert_eq(false, isThree([]))
    assert_eq(6, sumThree([1, 2, 3]))
    assert_eq(0, sumThree([1, 2]))
    42
}
