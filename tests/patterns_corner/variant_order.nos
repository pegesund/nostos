# expect: 42
# Order of variant patterns matters

type Num = Zero | Pos(Int) | Neg(Int)

classify(Zero) = "zero"
classify(Pos(1)) = "one"
classify(Pos(_)) = "positive"
classify(Neg(-1)) = "minus one"
classify(Neg(_)) = "negative"

main() = {
    assert_eq("zero", classify(Zero))
    assert_eq("one", classify(Pos(1)))
    assert_eq("positive", classify(Pos(5)))
    assert_eq("minus one", classify(Neg(-1)))
    assert_eq("negative", classify(Neg(-5)))
    42
}
