# expect: 42
# Variant containing tuple

type Point = Point2D(Int, Int) | Point3D(Int, Int, Int)

getX(Point2D(x, _)) = x
getX(Point3D(x, _, _)) = x

sumCoords(Point2D(x, y)) = x + y
sumCoords(Point3D(x, y, z)) = x + y + z

main() = {
    assert_eq(1, getX(Point2D(1, 2)))
    assert_eq(10, getX(Point3D(10, 20, 30)))
    assert_eq(3, sumCoords(Point2D(1, 2)))
    assert_eq(60, sumCoords(Point3D(10, 20, 30)))
    42
}
