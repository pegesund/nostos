# expect: 42
# Deeply nested tuple patterns

deep3(((a, b), (c, d))) = a + b + c + d
deep4((((a, b), c), d)) = a + b + c + d

extractDeep((((x, _), _), _)) = x

main() = {
    assert_eq(10, deep3(((1, 2), (3, 4))))
    assert_eq(10, deep4((((1, 2), 3), 4)))
    assert_eq(99, extractDeep((((99, 0), 0), 0)))
    42
}
