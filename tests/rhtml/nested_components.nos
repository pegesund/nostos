# expect: <div><div id="outer" data-component="true"><div><div id="inner" data-component="true"><p>Hello</p></div></div></div></div>
# Nested components render correctly

inner() = RHtml(p("Hello"))
outer() = RHtml(div([component("inner", inner)]))

main() = {
    result = RHtml(div([component("outer", outer)]))
    renderRHtml(result)
}
