# expect: "<html><body><h1>My Page</h1><div id=\"counter\" data-component=\"true\"><p>Count: 42</p></div></body></html>"

use stdlib.rhtml.{render}

main() = {
    count = 42

    page = RHtml(
        html([
            body([
                h1("My Page"),
                component("counter", p("Count: " ++ show(count)))
            ])
        ])
    )

    render(page)
}
