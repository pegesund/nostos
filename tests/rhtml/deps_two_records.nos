# expect: 2
# Deps tracking: two different reactive records should have 2 entries

reactive User = { name: String }
reactive Theme = { color: String }

main() = {
    user = User("Alice")
    theme = Theme("blue")
    userDisplay = () => RHtml(p(user.name))
    themeDisplay = () => RHtml(p(theme.color))
    result = RHtml(div([
        component("user-info", userDisplay),
        component("theme-info", themeDisplay)
    ]))
    Map.size(result.deps)
}
