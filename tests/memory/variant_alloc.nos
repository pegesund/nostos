# expect: 55
# Allocate many variants

type Number = Zero | Succ(Number)

to_int(n) =
    match n
        Zero -> 0
        Succ(prev) -> 1 + to_int(prev)
    end

make_number(n) =
    if n <= 0 then Zero
    else Succ(make_number(n - 1))

main() = to_int(make_number(10)) + to_int(make_number(20)) + to_int(make_number(25))
