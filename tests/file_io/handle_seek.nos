# File handle seek
# expect: true

main() = {
  path = "/tmp/nostos_test_handle_seek.txt"

  File.writeAll(path, "0123456789")

  handle = File.open(path, "r")
  # Seek to position 5
  File.seek(handle, 5, "start")
  # Read from position 5
  data = File.read(handle, 5)
  File.close(handle)
  File.remove(path)
  data == "56789"
}
