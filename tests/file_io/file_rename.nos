# Rename/move file
# expect: true

main() = {
  old = "/tmp/nostos_test_rename_old.txt"
  new = "/tmp/nostos_test_rename_new.txt"
  content = "Rename me!"

  File.writeAll(old, content)
  File.rename(old, new)

  # Old should not exist
  oldExists = File.exists(old)
  # New should exist with content
  data = File.readAll(new)
  File.remove(new)

  oldExists == false && data == content
}
