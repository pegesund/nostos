# expect: 0
# Same trait for multiple types

type Point = { x: Int, y: Int }
type Circle = { radius: Int }

trait Size size(self) -> Int end
Point: Size size(self) = 2 end
Circle: Size size(self) = 4 end

main() = {
    p = Point(0, 0)
    c = Circle(5)
    result = p.size() + c.size()
    assert_eq(6, result)
    0
}
