# expect: passed
# Order of constraints shouldn't matter
type Val = Val(Int) deriving (Hash + Eq + Show)

forwardCheck[T: Hash + Eq](x: T, y: T) -> Bool = x == y
reverseCheck[T: Eq + Hash](x: T, y: T) -> Bool = x == y

main() = {
    v1 = Val(42)
    v2 = Val(42)
    r1 = forwardCheck(v1, v2)
    r2 = reverseCheck(v1, v2)
    assert(r1 == r2)
    assert(r1 == true)
    "passed"
}
