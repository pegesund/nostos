# expect: passed
# Test bounds with recursive type - just Eq, not Hash (Hash has issues with recursive calls)
type List = Cons(Int, List) | Nil deriving Eq

equal[T: Eq](x: T, y: T) -> Bool = x == y

main() = {
    list1 = Cons(1, Cons(2, Nil))
    list2 = Cons(1, Cons(2, Nil))
    list3 = Cons(1, Nil)
    assert(equal(list1, list2) == true)
    assert(equal(list1, list3) == false)
    "passed"
}
