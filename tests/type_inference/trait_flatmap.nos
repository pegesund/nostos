# expect: 9
# Test: trait method in flatMap closure

trait Replicable
    duplicate(self) -> List[Int]
end

type Item2 = Item2(Int)

Item2: Replicable
    duplicate(self) = match self { Item2(n) -> [n, n, n] }
end

duplicateAll[T: Replicable](items: List[T]) -> List[Int] =
    flatten(map(items, (x) => x.duplicate()))

main() = {
    items = [Item2(1), Item2(2), Item2(3)]
    length(duplicateAll(items))
}
