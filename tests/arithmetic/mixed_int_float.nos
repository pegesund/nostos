# expect: 0
# Test compile-time int-to-float coercion for mixed numeric operations

main() = {
    # Int * Float = Float
    assert_eq(79.04, 8 * 9.88)
    assert_eq(79.04, 9.88 * 8)

    # Int + Float = Float
    assert_eq(10.5, 10 + 0.5)
    assert_eq(10.5, 0.5 + 10)

    # Int - Float = Float
    assert_eq(8.5, 10 - 1.5)
    assert_eq(-8.5, 1.5 - 10)

    # Float / Int = Float
    assert_eq(25.0, 100.0 / 4)
    assert_eq(25.0, 100 / 4.0)

    # Chained operations with mixed types
    assert_eq(15.0, 10 + 2.5 + 2.5)
    assert_eq(25.0, 5 * 2.5 * 2)

    # Nested expressions
    assert_eq(10.0, (5 + 2.5) + 2.5)
    assert_eq(17.5, 10 + (5 * 1.5))
    0
}
