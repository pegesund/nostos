# expect: ()
# Power operation edge cases (using floats since ** is for floats)

main() = {
    # 0^0 = 1 (mathematical convention)
    assert_eq(1.0, 0.0 ** 0.0)

    # Negative base with odd exponent
    assert_eq(-8.0, (-2.0) ** 3.0)

    # Negative base with even exponent
    assert_eq(4.0, (-2.0) ** 2.0)

    # 1^anything = 1
    assert_eq(1.0, 1.0 ** 1000.0)

    # 0^positive = 0
    assert_eq(0.0, 0.0 ** 5.0)
}
