# expect: 99
# Test that only y has a setter (x uses default which won't work on mutable record)
# This test verifies that partial setters work when present

# Define a mutable record
var type Point = { x: Int, y: Int }

# Only provide setter for y, not x
pointSetY(p: Point, v: Int) -> Point = {
    # Update y field
    Point(p.x, v)
}

main() = {
    p = Point(10, 20)

    # This should call pointSetY since setter exists
    p.y = 99

    p.y
}
