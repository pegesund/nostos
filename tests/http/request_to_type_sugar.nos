# expect: 42
# Test requestToType sugar: req.toType() with type annotation

type Result[T, E] = Ok(T) | Err(E)

type UserParams = { name: String, age: Int }

type HttpRequest = { id: Int, method: String, path: String, headers: List[(String, String)], body: String, queryParams: List[(String, String)], cookies: List[(String, String)], formParams: List[(String, String)], isWebSocket: Bool }

main() = {
    req = HttpRequest(1, "GET", "/test", [], "", [("name", "Alice"), ("age", "42")], [], [], false)

    # Sugar syntax: compiler transforms this to requestToType(req, "UserParams")
    result: Result[UserParams, String] = req.toType()

    match result {
        Ok(params) -> params.age
        Err(msg) -> -1
    }
}
