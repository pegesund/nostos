# Test: Merge two sorted lists
merge([], ys) = ys
merge(xs, []) = xs
merge([x | xs], [y | ys]) =
    if x <= y then [x] ++ merge(xs, [y | ys])
    else [y] ++ merge([x | xs], ys)

sumList([]) = 0
sumList([x | xs]) = x + sumList(xs)

main() = {
    a = [1, 3, 5, 7]
    b = [2, 4, 6, 8]
    merged = merge(a, b)
    sumList(merged)
}
