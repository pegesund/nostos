# Test: Right fold (reduce from right)
foldr(_, init, []) = init
foldr(f, init, [x | xs]) = f(x, foldr(f, init, xs))

main() = {
    nums = [1, 2, 3, 4]
    # Build right-associative: 1 - (2 - (3 - (4 - 0))) = 1 - (2 - (3 - 4)) = 1 - (2 - (-1)) = 1 - 3 = -2
    result = foldr((a, b) => a - b, 0, nums)
    result + 100
}
